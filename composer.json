{
	"type": "project",
	"license": "proprietary",
	"version": "2.9.1",
	"require": {
		"php": "^7.1.3",
		"ext-ctype": "*",
		"ext-gd": "^7.2",
		"ext-gmp": "^7.2",
		"ext-iconv": "^7.2",
		"ext-json": "^1.6",
		"ext-mysqli": "^7.2",
		"ext-pdo": "*",
		"ext-simplexml": "*",
		"beberlei/doctrineextensions": "^1.2",
		"bentools/webpush-bundle": "0.6.*",
		"dereuromark/media-embed": "^0.5.1",
		"doctrine/annotations": "^1.6",
		"doctrine/common": "2.13.*",
		"doctrine/doctrine-bundle": "2.1.0",
		"doctrine/doctrine-migrations-bundle": "^3.0",
		"doctrine/persistence": "1.3.7",
		"doctrine/sql-formatter": "^1.1",
		"enqueue/sqs": "^0.10.1",
		"erusev/parsedown": "^1.7",
		"gigadrive/symfony-extensions": "*@dev",
		"google/apiclient": "^2.7",
		"gumlet/php-image-resize": "^1.9",
		"guzzlehttp/guzzle": "^6.3",
		"jaybizzle/crawler-detect": "^1.2",
		"jms/serializer": "^3.2",
		"kreait/firebase-php": "^4.35",
		"league/oauth1-client": "^1.7",
		"sensio/framework-extra-bundle": "^5.1",
		"sentry/sentry-symfony": "^3.0",
		"sroze/messenger-enqueue-transport": "^0.5.0",
		"symfony/cache": "5.1.*",
		"symfony/console": "5.1.*",
		"symfony/dotenv": "5.1.*",
		"symfony/event-dispatcher": "5.1.*",
		"symfony/expression-language": "5.1.*",
		"symfony/flex": "^1.3.1",
		"symfony/framework-bundle": "5.1.*",
		"symfony/http-client": "5.1.*",
		"symfony/intl": "5.1.*",
		"symfony/messenger": "5.1.*",
		"symfony/monolog-bundle": "^3.4",
		"symfony/orm-pack": "*",
		"symfony/process": "5.1.*",
		"symfony/security-bundle": "5.1.*",
		"symfony/security-csrf": "5.1.*",
		"symfony/serializer-pack": "*",
		"symfony/swiftmailer-bundle": "^3.2",
		"symfony/translation": "5.1.*",
		"symfony/twig-bundle": "5.1.*",
		"symfony/validator": "5.1.*",
		"symfony/web-link": "5.1.*",
		"symfony/yaml": "5.1.*"
	},
	"require-dev": {
		"symfony/debug-pack": "^1.0",
		"symfony/maker-bundle": "^1.13",
		"symfony/profiler-pack": "*"
	},
	"config": {
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true
	},
	"autoload": {
		"files": [
			"src/global_functions.php"
		],
		"psr-4": {
			"qpost\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"qpost\\Tests\\": "tests/"
		}
	},
	"replace": {
		"paragonie/random_compat": "2.*",
		"symfony/polyfill-ctype": "*",
		"symfony/polyfill-iconv": "*",
		"symfony/polyfill-php71": "*",
		"symfony/polyfill-php70": "*",
		"symfony/polyfill-php56": "*"
	},
	"scripts": {
		"auto-scripts": {
			"cache:clear": "symfony-cmd",
			"assets:install %PUBLIC_DIR%": "symfony-cmd"
		},
		"post-install-cmd": [
			"@auto-scripts"
		],
		"post-update-cmd": [
			"@auto-scripts"
		]
	},
	"conflict": {
		"symfony/symfony": "*"
	},
	"extra": {
		"symfony": {
			"allow-contrib": true,
			"require": "5.1.*"
		}
	}
}
